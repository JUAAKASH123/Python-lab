dict1={ }
for i in range(0,4):
    key=input("Enter the key")
    value=input("Enter the value")
    dict1[key]=value
print("The dictionary in ascending order",dict(sorted(dict1.items())))
print("The dictionary in descenting order",(dict(sorted(dict1.items(),reverse=True))))
dict={"key1":1,"key2":2}
print(dict)
